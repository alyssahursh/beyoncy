<table>
  <tr>
    <th>Order Number</th>
    <th>Order Status</th>
    <th>Order Created</th>
  </tr>

  <% @orders.each do |order| %>
  <tr>
    <td><%= link_to("Order ##{order.id}", order_path(order)) %>    </td>
    <td><%= order.order_status %></td>
    <td><%= order.created_at %></td>
  </tr>

  <% end %>
</table>
